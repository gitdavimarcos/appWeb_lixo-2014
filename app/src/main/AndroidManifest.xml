        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.TelaLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.TelaPrincipal" />
        <activity android:name=".activity.TelaInscricao" />
        <activity android:name=".activity.TelaProfessor" />
        <activity android:name=".activity.TelaCurso" />
        <activity android:name=".activity.Turma" />
        <activity android:name=".activity.TelaLancarNota"></activity>
    </application>

</manifest>